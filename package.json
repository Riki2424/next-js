        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "eslint": "8.32.0",
        "eslint-config-next": "13.1.4",
        "eslint-config-prettier": "8.6.0",
        "prettier": "2.8.3",
        "typescript": "4.9.4",
        "yarn-run-all": "^3.1.1"
    }
}
